		"devdoc": {
			"details": "A simple digital vending machine contract that allows the owner to manage items and users to purchase them. The DigitalVendingMachine smart contract is a straightforward implementation of a vending machine on the Ethereum blockchain. It allows the contract owner to manage a list of items available for purchase and enables users to buy these items by sending Ether.",
			"kind": "dev",
			"methods": {
				"addItem(string,uint256,uint256)": {
					"details": "Add a new item to the vending machine.",
					"params": {
						"name": "The name of the item.",
						"price": "The price of the item in wei.",
						"quantity": "The initial stock quantity of the item."
					}
				},
				"getItem(uint256)": {
					"details": "Retrieve item details by index.",
					"params": {
						"index": "The index of the item in the array."
					},
					"returns": {
						"id": "The ID of the item.",
						"name": "The name of the item.",
						"price": "The price of the item in wei.",
						"quantity": "The available quantity of the item."
					}
				},
				"getItemCount()": {
					"details": "Get the total number of items in the vending machine.",
					"returns": {
						"_0": "The number of items."
					}
				},
				"purchaseItem(uint256,uint256)": {
					"details": "Purchase an item from the vending machine.",
					"params": {
						"id": "The ID of the item to purchase.",
						"quantity": "The number of items to purchase."
					}
				},
				"removeItem(uint256)": {
					"details": "Remove an item from the vending machine by its ID.",
					"params": {
						"id": "The ID of the item to remove."
					}
				},
				"transferOwnership(address)": {
					"details": "Transfer contract ownership to a new address.",
					"params": {
						"newOwner": "The address to transfer ownership to."
					}
				},
				"withdrawFunds()": {
					"details": "Withdraw all Ether from the contract to the owner's address."
				}
			},
